{
  "name": "prisma-kysely",
  "description": "Generate Kysely database types from a Prisma schema",
  "repository": {
    "url": "https://github.com/valtyr/prisma-kysely"
  },
  "version": "1.0.7",
  "main": "dist/generator.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "bin": {
    "prisma-kysely": "dist/bin.js"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "tsc --watch",
    "build": "tsc",
    "typecheck": "tsc --noemit",
    "prepack": "yarn build",
    "fix": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "eslint ./src",
    "test": "jest --passWithNoTests --coverage --collectCoverageFrom=\"./src/**/*.ts\""
  },
  "dependencies": {
    "@prisma/generator-helper": "4.11.0",
    "@prisma/internals": "4.11.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "29.5.0",
    "@types/node": "17.0.21",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "jest": "29.5.0",
    "kysely": "^0.23.5",
    "prettier": "^2.8.6",
    "prisma": "4.11.0",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  }
}
